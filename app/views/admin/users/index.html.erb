管理者画面

<table class="table">
  <tr>
    <th>名前</th>
    <th>email</th>
    <th>pasword</th>

  </tr>

  <% @users.each do |user| %>
  <tr>
    <td class="admin_user_id"><%= user.id %></td>
    <td class="admin_user_name"><%= user.name %></td>
    <td class="admin_user_finish"><%= user.email %></td>
    <td class="admin_user_status"><%= user.password %></td>
    <td class="admin_user_created"><%= user.created_at %></td>
    <td class="admin_user_show"><%= link_to '詳細を確認する', admin_user_path(user.id) %></td>
    <td class="admin_user_edit"><%= link_to "ユーザーを編集する", edit_admin_user_path(user.id) %></td>
    <td class="admin_user_destroy"><%= link_to 'ユーザーを削除する', admin_user_path(user.id), method: :delete ,data: { confirm: '本当に削除していいですか？' } %></td>
  </tr>
<% end %>
</table>

<%= paginate @users, theme: 'twitter-bootstrap-4' %>

<%= link_to "ユーザー作成", new_admin_user_path %>